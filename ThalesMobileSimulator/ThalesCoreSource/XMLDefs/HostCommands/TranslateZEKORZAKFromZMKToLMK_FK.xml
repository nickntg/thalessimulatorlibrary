<?xml version="1.0" encoding="utf-8" ?>
<CommandConfiguration xmlns="http://tempuri.org/HostCommandDefinitions.xsd">
  <CommandName>Translates a ZEK or ZAK from encryption under a ZMK to encryption under LMK.</CommandName>
  <Request>FK</Request>
  <Response>FL</Response>

  <Field>
    <Name>Flag</Name>
    <Length>1</Length>
    <Type>Numeric</Type>
  </Field>
  
  <Field>
    <Name>ZMK</Name>
    <IncludeFile>MultiFormatZMK.xml</IncludeFile>
  </Field>

  <Field>
    <Name>Key</Name>
    <IncludeFile>MultiFormatKey.xml</IncludeFile>
  </Field>

  <Field>
    <Name>Delimiter</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <ValidValue>;</ValidValue>
  </Field>

  <Field>
    <Name>Reserved</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <DependentField>Delimiter</DependentField>
    <DependentValue>;</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

  <Field>
    <Name>Key Scheme LMK</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <DependentField>Delimiter</DependentField>
    <DependentValue>;</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

  <Field>
    <Name>Key Check Value Type</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <DependentField>Delimiter</DependentField>
    <DependentValue>;</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

</CommandConfiguration>
