<?xml version="1.0" encoding="utf-8" ?>
<CommandConfiguration xmlns="http://tempuri.org/HostCommandDefinitions.xsd">
  <CommandName>Verifies a terminal PIN using the VISA algorithm.</CommandName>
  <Request>DC</Request>
  <Response>DD</Response>

  <Field>
    <Name>TPK</Name>
    <IncludeFile>MultiFormatKey.xml</IncludeFile>
  </Field>

  <Field>
    <Name>PVK</Name>
    <IncludeFile>MultiFormatCVK.xml</IncludeFile>
  </Field>

  <Field>
    <Name>PIN Block</Name>
    <Length>16</Length>
    <Type>Hexadecimal</Type>
  </Field>

  <Field>
    <Name>PIN Block Format Code</Name>
    <Length>2</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>Account Number</Name>
    <Length>12</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>PVKI</Name>
    <Length>1</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>PVV</Name>
    <Length>4</Length>
    <Type>Numeric</Type>
  </Field>

</CommandConfiguration>
