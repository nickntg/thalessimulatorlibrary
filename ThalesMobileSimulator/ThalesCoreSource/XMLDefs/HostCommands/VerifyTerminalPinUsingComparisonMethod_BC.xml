<?xml version="1.0" encoding="utf-8" ?>
<CommandConfiguration xmlns="http://tempuri.org/HostCommandDefinitions.xsd">
  <CommandName>Verify a terminal PIN by comparing it with a value held on the Host database</CommandName>
  <Request>BC</Request>
  <Response>BD</Response>

  <Field>
    <Name>TPK</Name>
    <IncludeFile>MultiFormatKey.xml</IncludeFile>
  </Field>

  <Field>
    <Name>PIN Block</Name>
    <Length>16</Length>
    <Type>Hexadecimal</Type>
  </Field>

  <Field>
    <Name>PIN Block Format Code</Name>
    <Length>2</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>Account Number</Name>
    <Length>12</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>PIN</Name>
    <Length>CLEAR_PIN_LENGTH</Length>
    <Type>Hexadecimal</Type>
  </Field>

</CommandConfiguration>
