<?xml version="1.0" encoding="utf-8" ?>
<CommandConfiguration xmlns="http://tempuri.org/HostCommandDefinitions.xsd">
  <CommandName>Translates a PIN block from ZPK to ZPK encryption.</CommandName>
  <Request>CC</Request>
  <Response>CD</Response>

  <Field>
    <Name>Source ZPK</Name>
    <IncludeFile>MultiFormatKey.xml</IncludeFile>
  </Field>

  <Field>
    <Name>Destination ZPK</Name>
    <IncludeFile>MultiFormatKey.xml</IncludeFile>
  </Field>

  <Field>
    <Name>Max PIN Length</Name>
    <Length>2</Length>
    <Type>Numeric</Type>
    <ValidValue>12</ValidValue>
    <RejectionCodeIfInvalid>15</RejectionCodeIfInvalid>
  </Field>

  <Field>
    <Name>Source PIN Block</Name>
    <Length>16</Length>
    <Type>Hexadecimal</Type>
  </Field>

  <Field>
    <Name>Source PIN Block Format Code</Name>
    <Length>2</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>Destination PIN Block Format Code</Name>
    <Length>2</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>Account Number</Name>
    <Length>12</Length>
    <Type>Numeric</Type>
  </Field>

</CommandConfiguration>
