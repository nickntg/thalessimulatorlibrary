<?xml version="1.0" encoding="utf-8" ?>
<!-- This file includes the necessary definitions that parse
     a PIN block format code. -->
<PINBlockFormat>

  <Field>
    <Name>#replace#</Name>
    <Length>2</Length>
    <Type>Numeric</Type>
    <ValidValue>01</ValidValue>
    <ValidValue>02</ValidValue>
    <ValidValue>03</ValidValue>
    <ValidValue>04</ValidValue>
  </Field>

</PINBlockFormat>