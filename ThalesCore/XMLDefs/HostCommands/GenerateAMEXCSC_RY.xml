<?xml version="1.0" encoding="utf-8" ?>
<CommandConfiguration xmlns="http://tempuri.org/HostCommandDefinitions.xsd">
  <CommandName>Generates AMEX CSC.</CommandName>
  <Request>RY</Request>
  <Response>RZ</Response>

  <Field>
    <Name>Mode</Name>
    <Length>1</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>Flag</Name>
    <Length>1</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>CSCK</Name>
    <IncludeFile>MultiFormatCVK.xml</IncludeFile>
  </Field>

  <Field>
    <Name>Account Number</Name>
    <Length>19</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>Expiration Date</Name>
    <Length>4</Length>
    <Type>Numeric</Type>
  </Field>

  <Field>
    <Name>Service Code</Name>
    <Length>3</Length>
    <Type>Numeric</Type>
    <DependentField>Flag</DependentField>
    <DependentValue>2</DependentValue>
    <ExclusiveDependency>true</ExclusiveDependency>
  </Field>

  <Field>
    <Name>5CSC</Name>
    <Length>5</Length>
    <Type>Character</Type>
    <DependentField>Mode</DependentField>
    <DependentValue>4</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

  <Field>
    <Name>4CSC</Name>
    <Length>4</Length>
    <Type>Character</Type>
    <DependentField>Mode</DependentField>
    <DependentValue>4</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

  <Field>
    <Name>3CSC</Name>
    <Length>3</Length>
    <Type>Character</Type>
    <DependentField>Mode</DependentField>
    <DependentValue>4</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

</CommandConfiguration>
