<?xml version="1.0" encoding="utf-8" ?>
<CommandConfiguration xmlns="http://tempuri.org/HostCommandDefinitions.xsd">
  <CommandName>Translates a ZMK from encryption under a ZMK to encryption under LMK.</CommandName>
  <Request>BY</Request>
  <Response>BZ</Response>

  <Field>
    <Name>ZMK</Name>
    <IncludeFile>MultiFormatZMK.xml</IncludeFile>
  </Field>

  <Field>
    <Name>Key</Name>
    <IncludeFile>MultiFormatZMK.xml</IncludeFile>
  </Field>

  <Field>
    <Name>Atalla Variant 1</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <OptionValue>0</OptionValue>
    <OptionValue>1</OptionValue>
    <OptionValue>2</OptionValue>
    <OptionValue>3</OptionValue>
    <OptionValue>4</OptionValue>
    <OptionValue>5</OptionValue>
    <OptionValue>6</OptionValue>
    <OptionValue>7</OptionValue>
    <OptionValue>8</OptionValue>
    <OptionValue>9</OptionValue>
  </Field>

  <Field>
    <Name>Atalla Variant 2</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <OptionValue>0</OptionValue>
    <OptionValue>1</OptionValue>
    <OptionValue>2</OptionValue>
    <OptionValue>3</OptionValue>
    <OptionValue>4</OptionValue>
    <OptionValue>5</OptionValue>
    <OptionValue>6</OptionValue>
    <OptionValue>7</OptionValue>
    <OptionValue>8</OptionValue>
    <OptionValue>9</OptionValue>
  </Field>

  <Field>
    <Name>Delimiter</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <ValidValue>;</ValidValue>
  </Field>

  <Field>
    <Name>Reserved</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <DependentField>Delimiter</DependentField>
    <DependentValue>;</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

  <Field>
    <Name>Key Scheme LMK</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <DependentField>Delimiter</DependentField>
    <DependentValue>;</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

  <Field>
    <Name>Key Check Value Type</Name>
    <Length>1</Length>
    <Type>Character</Type>
    <DependentField>Delimiter</DependentField>
    <DependentValue>;</DependentValue>
    <ExclusiveDependency>false</ExclusiveDependency>
  </Field>

</CommandConfiguration>
